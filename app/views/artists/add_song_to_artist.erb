<h1>Add a song for this artist: </h1>

<form action="/artists/<%=@artist.id%>/songs" method="POST">
    <% @songs.each do |song| %>
        <input type="checkbox" name="artist[song_ids][]" id="<%= song.id %>" value="<%= song.id %>"><%= song.title %><br>
        <% end %>
        <input type="submit" value="Add song">
</form>